{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "unibest",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#1296db",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "white",
    "height": "55px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_active.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/phone.png",
        "selectedIconPath": "static/tabbar/phone_active.png",
        "pagePath": "pages/cell-center/index",
        "text": "客服"
      },
      {
        "iconPath": "static/tabbar/user.png",
        "selectedIconPath": "static/tabbar/user_active.png",
        "pagePath": "pages/mine/index",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/appointment/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "预约报闸"
      }
    },
    {
      "path": "pages/cell-center/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "客服"
      }
    },
    {
      "path": "pages/mine/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/my-account/edit",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "编辑账户信息",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my-account/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "我的账户",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/report-info/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "报闸信息"
      }
    }
  ],
  "subPackages": []
}